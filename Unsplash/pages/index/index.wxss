.card{
  margin: 5px 0;
}
.swipeout .i-swipeout-demo-item{
  border-bottom: none;
  padding: 0;
  margin: 0;
}
.swipeout .i-swipeout-item{
  margin: 0;
  padding: 0;
}
.process{
  display: block
}
/* 回到顶部图片 */
.goUp{  
  z-index: 1000;
  display: inline;
  position: fixed;
  bottom: 10%;
  right: 10%;
  width: 40rpx;
  height: 40rpx; 
}
/* 搜索框图片 */
.research{
  z-index: 1000;
  display: inline;
  position: fixed;
  bottom: 10%;
  left: 4%;
  width: 40rpx;
  height: 40rpx; 
}
/* 搜索框 */
.input{
  z-index: 1000;
  display: inline;
  position: fixed;
  top: 40%;
  width: 100%;
  color: #fafafa;
}
/* 蒙层 */
.mask{
  width:100%;
  height:100%;
  position:fixed;
  background-color:#999;
  z-index: 100;
  top:0;
  left:0;
  opacity:0.5;
  -moz-opacity: 0.7;  
  opacity: 0.70;  
  filter: alpha(opacity=70);
}